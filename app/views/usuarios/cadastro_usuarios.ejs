                    <div class="col-md-12 text-center">
                      <h3 class="card-title text-white">Cadastrar usuários</h3>
                    </div>
                  </div>
                </div>
                <div class="card-body">
                  <div class="row">
                    <div class="col-md-6">
                      <h3 class="text-center">Cadastrar usuário</h3>
                      <form action="/efetuar_cadastro_usuario" method="POST" enctype="MULTIPART/FORM-DATA">
                        <%if (validacaoUsuario.length > 0) {%>
                          <%for (let i = 0; i < validacaoUsuario.length; i++) {%>
                            <%if (validacaoUsuario[i].msg == "0") {%>
                              <span class="text-danger">Campo vazio!</span>
                            <%}%>
                            <%if (validacaoUsuario[i].msg == "1") {%>
                              <span class="text-danger">Tamanho inválido!</span>
                            <%}%>   
                          <%}%>  
                        <%}%>
                        <div class="form-group">  
                          <label class="bmd-label-floating">Nome completo</label>
                          <input type="text" class="form-control" name="nome_usuario_cadastro"/>
                        </div>
                        <%if (validacaoUsuario.length > 0) {%>
                          <%for (let i = 0; i < validacaoUsuario.length; i++) {%>
                            <%if (validacaoUsuario[i].msg == "2") {%>
                              <span class="text-danger">Campo vazio!</span>
                            <%}%>  
                          <%}%>  
                        <%}%>
                        <%if (validacaoCpfUsuario.length > 0) {%>
                          <%if (validacaoCpfUsuario == "0") {%>
                            <span class="text-danger">CPF inválido!</span>
                          <%}%>  
                        <%}%>  
                        <div class="form-group">
                          <label class="bmd-label-floating">CPF</label>
                          <input type="text" class="form-control" name="cpf_usuario_cadastro"/>
                        </div>
                        <%if (validacaoUsuario.length > 0) {%>
                          <%for (let i = 0; i < validacaoUsuario.length; i++) {%>
                            <%if (validacaoUsuario[i].msg == "3") {%>
                              <span class="text-danger">Campo vazio!</span>
                            <%}%>  
                            <%if (validacaoUsuario[i].msg == "4") {%>
                              <span class="text-danger">Tamanho inválido!</span>
                            <%}%>
                            <%if (validacaoUsuario[i].msg == "5") {%>
                              <span class="text-danger">E-mail inválido!</span>
                            <%}%>
                          <%}%>  
                        <%}%>
                        <div class="form-group">
                          <label class="bmd-label-floating">E-mail</label>
                          <input type="text" class="form-control" name="email_usuario_cadastro"/>
                        </div>
                        <%if (validacaoUsuario.length > 0) {%>
                          <%for (let i = 0; i < validacaoUsuario.length; i++) {%>
                            <%if (validacaoUsuario[i].msg == "6") {%>
                              <span class="text-danger">Campo vazio!</span>
                            <%}%>  
                            <%if (validacaoUsuario[i].msg == "7") {%>
                              <span class="text-danger">Tamanho inválido!</span>
                            <%}%>
                            <%if (validacaoUsuario[i].msg == "8") {%>
                              <span class="text-danger">E-mail inválido!</span>
                            <%}%>
                          <%}%>  
                        <%}%>
                        <%if (validacaoEmailUsuario.length > 0) {%>
                          <%if (validacaoEmailUsuario == "1") {%>
                            <span class="text-danger"> Os e-mails não são iguais!</span>
                          <%}%>  
                        <%}%>  
                        <div class="form-group">
                          <label class="bmd-label-floating">Confirmar e-mail</label>
                          <input type="text" class="form-control" name="confirmar_email_usuario_cadastro"/>
                        </div>
                        <%if (validacaoUsuario.length > 0) {%>
                          <%for (let i = 0; i < validacaoUsuario.length; i++) {%>
                            <%if (validacaoUsuario[i].msg == "9") {%>
                              <span class="text-danger">Campo vazio!</span>
                            <%}%>  
                            <%if (validacaoUsuario[i].msg == "10") {%>
                              <span class="text-danger">Tamanho inválido!</span>
                            <%}%>
                          <%}%>  
                        <%}%>
                        <div class="form-group">
                          <label class="bmd-label-floating">Senha</label>
                          <input type="password" class="form-control" name="senha_usuario_cadastro"/>
                        </div>
                        <%if (validacaoUsuario.length > 0) {%>
                          <%for (let i = 0; i < validacaoUsuario.length; i++) {%>
                            <%if (validacaoUsuario[i].msg == "11") {%>
                              <span class="text-danger">Campo vazio!</span>
                            <%}%>  
                            <%if (validacaoUsuario[i].msg == "12") {%>
                              <span class="text-danger">Tamanho inválido!</span>
                            <%}%>
                          <%}%>  
                        <%}%>
                        <%if (validacaoSenhaUsuario.length > 0) {%>
                          <%if (validacaoSenhaUsuario == "2") {%>
                            <span class="text-danger">As senhas não são iguais!</span>
                          <%}%>  
                        <%}%> 
                        <div class="form-group">
                          <label class="bmd-label-floating">Confirmar senha</label>
                          <input type="password" class="form-control" name="confirmar_senha_usuario_cadastro"/>
                        </div>
                        <%if (validacaoUsuario.length > 0) {%>
                          <%for (let i = 0; i < validacaoUsuario.length; i++) {%>
                            <%if (validacaoUsuario[i].msg == "13") {%>
                              <span class="text-danger">O nível de usuário é obrigatório!</span>
                            <%}%>  
                          <%}%>  
                        <%}%>  
                        <div class="form-group">
                          <h4>Nível de usuário</h4>
                          <select class="custom-select" name="nivel_usuario">
                            <option value="">ESCOLHER</option>
                            <option value="2">USUÁRIO COMUM</option>
                          </select>
                        </div>
                        <%if (validacaoImgUsuario.length > 0) {%>
                          <%if (validacaoImgUsuario == "0") {%>
                            <span class="text-danger">A imagem é obrigatória!</span>
                          <%}%>  
                        <%}%> 
                        <h4 class="mt-2">Foto de perfil</h4>
                        <input class="mt-4" type="file" id="id_usuario_imagem" name="imagem_usuario" accept=""/>
                        <label id="id_label_usuario" for="id_usuario_imagem">
                          <i class="material-icons">insert_photo</i>&nbsp;ESCOLHER FOTO</label>
                        <div class="form-group">
                          <a href="/lista_usuarios" class="btn btn-danger col-md-12">CANCELAR</a>
                          <button type="submit" class="btn btn-success col-md-12">CADASTRAR</button>
                        </div>
                      </form>
                    </div>
                    <div class="col-md-6">
