                        <%if (validacaoEmailAdm.length > 0) {%>
                          <%if (validacaoEmailAdm == "1") {%>
                            <span class="text-danger"> Os e-mails não são iguais!</span>
                          <%}%>  
                        <%}%>  
                        <div class="form-group">
                          <label class="bmd-label-floating">Confirmar e-mail</label>
                          <input type="text" class="form-control" name="confirmar_email_adm_cadastro"/>
                        </div>
                        <%if (validacaoAdm.length > 0) {%>
                          <%for (let i = 0; i < validacaoAdm.length; i++) {%>
                            <%if (validacaoAdm[i].msg == "9") {%>
                              <span class="text-danger">Campo vazio!</span>
                            <%}%>  
                            <%if (validacaoAdm[i].msg == "10") {%>
                              <span class="text-danger">Tamanho inválido!</span>
                            <%}%>
                          <%}%>  
                        <%}%>
                        <div class="form-group">
                          <label class="bmd-label-floating">Senha</label>
                          <input type="password" class="form-control" name="senha_adm_cadastro"/>
                        </div>
                        <%if (validacaoAdm.length > 0) {%>
                          <%for (let i = 0; i < validacaoAdm.length; i++) {%>
                            <%if (validacaoAdm[i].msg == "11") {%>
                              <span class="text-danger">Campo vazio!</span>
                            <%}%>  
                            <%if (validacaoAdm[i].msg == "12") {%>
                              <span class="text-danger">Tamanho inválido!</span>
                            <%}%>
                          <%}%>  
                        <%}%>
                        <%if (validacaoSenhaAdm.length > 0) {%>
                          <%if (validacaoSenhaAdm == "2") {%>
                            <span class="text-danger">As senhas não são iguais!</span>
                          <%}%>  
                        <%}%>  
                        <div class="form-group">
                          <label class="bmd-label-floating">Confirmar senha</label>
                          <input type="password" class="form-control" name="confirmar_senha_adm_cadastro"/>
                        </div>
                        <%if (validacaoAdm.length > 0) {%>
                          <%for (let i = 0; i < validacaoAdm.length; i++) {%>
                            <%if (validacaoAdm[i].msg == "13") {%>
                              <span class="text-danger">O nível de usuário é obrigatório!</span>
                            <%}%>  
                          <%}%>  
                        <%}%> 
                        <div class="form-group">
                          <h4>Nível de usuário</h4>
                          <select class="custom-select" name="nivel_adm">
                            <option value="">ESCOLHER</option>
                            <option value="1">ADMINISTRADOR</option>
                          </select>
                        </div>
                        <%if (validacaoImgAdm.length > 0) {%>
                          <%if (validacaoImgAdm == "0") {%>
                            <span class="text-danger">A imagem é obrigatória!</span>
                          <%}%>  
                        <%}%>  
                        <h4 class="mt-2">Foto de perfil</h4>
                        <input class="mt-4" type="file" id="id_adm_imagem" name="imagem_adm" accept=""/>
                        <label id="id_label_adm" for="id_adm_imagem">
                          <i class="material-icons">insert_photo</i>&nbsp;ESCOLHER FOTO</label>
                        <div class="form-group">
                          <a href="/lista_usuarios" class="btn btn-danger col-md-12">CANCELAR</a>
                          <button type="submit" class="btn btn-success col-md-12">CADASTRAR</button>
                        </div>
                      </form>
                    </div>
                  </div>
                </div>
